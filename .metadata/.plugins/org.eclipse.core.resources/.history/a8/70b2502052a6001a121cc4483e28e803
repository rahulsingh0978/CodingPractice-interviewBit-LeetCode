package leetcode;

public class ReverseWordString {
	public void reverseWords(String s) {
		int i = 0;
		int j = s.length() - 1;
		while (i < j) {
			if (s.charAt(i) == ' ') {
				i++;
			}
			if (s.charAt(j) == ' ') {
				j--;
			}
		}
		for(int k=i;k<=j;k++) {
			System.out.print(s.charAt(i));
		}
	}
	public static void main(String[] args) {
		ReverseWordString obj = new ReverseWordString();
		obj.reverseWords("  hello world!  ");;
	}
}
