package leetcode;

public class NFactorial {
	 public int trailingZeroes(int n) {
	        if(n==0 ||n==1){
	            return 0;
	        }
	         int count =0;
	        int v = 5;
	        int value = n/v;
	        while(value >0){
	            count += value ;
	            v *=v;
	            value = n/v;
	        }
	        return count;
	        
	    }
	 public static void main(String[] args) {
		 NFactorial obj = new NFactorial();
		 System.out.println(obj.trailingZeroes(200));
		 System.out.println(200/625);
	}
}
