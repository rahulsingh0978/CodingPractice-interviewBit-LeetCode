package interviewBits;

import java.util.Stack;

public class LongestParenthesis {
	public int longestValidParentheses(String A) {
		int maxlen;
		Stack<Character> stack  = new  Stack();
		stack.push(-1);
		for(int i=0;i<A.length();i++) {
			if(A.charAt(i)=='(') {
				stack.push(i);
			}
			
		}
    }
	
}
